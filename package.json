{
  "name": "auth-remix",
  "description": "Authentication for remix.",
  "version": "0.1.3",
  "type": "module",
  "files": [
    "dist",
    "src",
    "!vitest.config.js"
  ],
  "exports": {
    "./providers": {
      "types": "./dist/providers/index.d.ts"
    },
    "./providers/*": {
      "types": "./dist/providers/*.d.ts",
      "import": "./dist/providers/*.js"
    },
    "./cloudflare": {
      "types": "./dist/cloudflare/index.d.ts",
      "import": "./dist/cloudflare/index.js"
    },
    "./deno": {
      "types": "./dist/deno/index.d.ts",
      "import": "./dist/deno/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm clean && tsc",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "remix",
    "oauth",
    "jwt",
    "oauth2",
    "authentication",
    "csrf",
    "oidc",
    "Auth.js"
  ],
  "author": "",
  "contributors": [],
  "repository": "https://github.com/hambergerpls/auth-remix",
  "license": "ISC",
  "dependencies": {
    "@auth/core": "^0.37.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.7",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "peerDependencies": {
    "@remix-run/cloudflare": ">=2.13.1",
    "@remix-run/deno": ">=2.13.1",
    "@remix-run/node": ">=2.13.1",
    "cookie": ">=0.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
